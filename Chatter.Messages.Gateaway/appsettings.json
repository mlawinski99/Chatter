{
  "ReverseProxy": {
    "Routes": {
      "messagesRoute": {
        "ClusterId": "messagesCluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "messagesRoute": {
        "Destinations": {
          "messages": {
            "Address": "http://localhost:8082/"
          }
        }
      }
    }
  }
}
