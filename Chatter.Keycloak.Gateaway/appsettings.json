{
  "ReverseProxy": {
    "Routes": {
      "keycloakRoute": {
        "ClusterId": "keycloakCluster",
        "Match": {
          "Path": "/auth/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "keycloakCluster": {
        "Destinations": {
          "keycloak": {
            "Address": "http://localhost:8080/"
          }
        }
      }
    }
  }
}
